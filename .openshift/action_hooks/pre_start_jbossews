export _JAVA_OPTIONS="$_JAVA_OPTIONS -Dspring.profiles.active=prod"
export _JAVA_OPTIONS="$_JAVA_OPTIONS -DMYLOGDIR=$OPENSHIFT_DATA_DIR/logs/"
export _JAVA_OPTIONS="$_JAVA_OPTIONS -Drd.config.file=$OPENSHIFT_DATA_DIR/config/config_prod.properties"
export JAVA_OPTIONS="$JAVA_OPTIONS -Dspring.profiles.active=prod"
export JAVA_OPTIONS="$JAVA_OPTIONS -DMYLOGDIR=$OPENSHIFT_DATA_DIR/logs/"
export JAVA_OPTIONS="$JAVA_OPTIONS -Drd.config.file=$OPENSHIFT_DATA_DIR/config/config_prod.properties"
export JAVA_OPTS="$JAVA_OPTS -Dspring.profiles.active=prod"
export JAVA_OPTS="$JAVA_OPTS -DMYLOGDIR=$OPENSHIFT_DATA_DIR/logs/"
export JAVA_OPTS="$JAVA_OPTS -Drd.config.file=$OPENSHIFT_DATA_DIR/config/config_prod.properties"